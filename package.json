{
  "name": "medai-advanced",
  "version": "2.0.0",
  "description": "Advanced AI-powered medical symptom analyzer combining multiple biomedical models (BioGPT, ClinicalBERT, NER) for intelligent health assessment. Features real-time symptom analysis, urgency scoring, clinical recommendations, and professional medical-grade UI. Built with React, FastAPI, and deployed on Firebase.",
  "keywords": [
    "medical",
    "healthcare",
    "ai",
    "symptom-analysis",
    "biomedical",
    "clinical",
    "machine-learning",
    "healthcare-technology"
  ],
  "homepage": "https://ai-symptom-analyzer.web.app",
  "bugs": {
    "url": "https://github.com/faizanshoukat5/Ai-symptom-analyzer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/faizanshoukat5/Ai-symptom-analyzer.git"
  },
  "license": "MIT",
  "author": {
    "name": "MedAI Advanced Team",
    "email": "contact@medai-advanced.com",
    "url": "https://medai-advanced.com"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "echo \"Add tests here\" && exit 0",
    "test:integration": "python test_frontend_integration.py",
    "health-check": "python health_check.py",
    "start:backend": "cd backend && python main_advanced_models.py",
    "start:full": "concurrently \"npm run start:backend\" \"npm run dev\"",
    "build:production": "npm run lint && npm run build",
    "clean": "rm -rf dist node_modules/.cache",
    "setup": "npm install && cd backend && pip install -r requirements_advanced.txt",
    "firebase:init": "firebase init",
    "firebase:deploy": "npm run build && firebase deploy",
    "firebase:deploy:hosting": "npm run build && firebase deploy --only hosting",
    "firebase:deploy:functions": "firebase deploy --only functions",
    "firebase:serve": "npm run build && firebase serve"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/postcss": "^4.1.11",
    "axios": "^1.6.0",
    "chart.js": "^4.5.0",
    "date-fns": "^2.30.0",
    "firebase": "^11.10.0",
    "framer-motion": "^12.23.0",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.8.0",
    "recharts": "^2.8.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "terser": "^5.43.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0"
  }
}
